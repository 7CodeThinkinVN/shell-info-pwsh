{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#017396",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "foreground": "#017396",
          "properties": {
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\ue62a "
          },
          "style": "powerline",
          "template": "[{{ if .WSL }}WSL at {{ end }}{{.Icon}}]",
          "type": "os"
        },
        {
          "foreground": "#e9c9a0",
          "style": "powerline",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": "[{{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}]",
          "type": "git"
        },
        {
          "foreground": "#1caa09",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "[\ue5ff  {{ .Path  }}]",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#5c6b94",
          "leading_diamond": "<#5c6b94>\u2570\u2500</>",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": "[\ue641 {{ .CurrentDate | date .Format }}]",
          "type": "time"
        },
        {
          "foreground": "#9852c4",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": "[\uf252 {{ .FormattedMs }}]",
          "type": "executiontime"
        },
        {
          "foreground": "#28d6b0",
          "style": "powerline",
          "template": "[\ue266  MEM: {{ round .PhysicalPercentUsed .Precision }}% |RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB]",
          "type": "sysinfo"
        },
        {
          "foreground": "#c73980",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " \u2665 <#eb7fb5> \ue285 ",
          "type": "exit"
        },
        {
          "style": "plain",
          "template": " \u279c ",
          "type": "root"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "template": "<#c73980>\u2665 <#ff99ff>U<#ff99cc>w<#ff99ff>U<#c73980>\u2665 <#eb7fb5> \ue285 "
  },
  "version": 2
}
